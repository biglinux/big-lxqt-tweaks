pkgname=big-lxqt-tweaks
pkgver=$(date +%y.%m.%d)
pkgrel=$(date +%H%M)
pkgdesc="Tweaks for LXQT"
arch=('any')
url="https://github.com/biglinux/$pkgname"
license=('GPL3')
# depends=('')
provides=("$pkgname")
source=("git+${url}.git")
sha256sums=('SKIP')

package() {
    cp -r "${srcdir}/${pkgname}/etc/" "${pkgdir}/"
    cp -r "${srcdir}/${pkgname}/usr/" "${pkgdir}/"
    
    }
